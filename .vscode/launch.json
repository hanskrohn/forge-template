{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch main.go",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal"
    },
    {
      "name": "Launch Create File command",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["create-file", "-t", "temo", "-f", "User.tsx"],
      "console": "integratedTerminal"
    }
  ]
}
